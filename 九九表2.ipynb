    "## 1 から n までの九九表を作成する"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "import io\n",
    "import sys\n",
    "\n",
    "# 1 から n までの九九表を作成する\n",
    "def print_multiplication_table(n):\n",
    "    for i in range(1, n + 1):\n",
    "        for j in range(1, n + 1):\n",
    "            print(\"{:4d}\".format(i * j), end=\"\")\n",
    "        print()\n",
    "\n",
    "print_multiplication_table(30)"
   ]
